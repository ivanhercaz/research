@article{messinaConsiderationsPlanningVegan2001,
  title = {Considerations in {{Planning Vegan Diets}}: {{Children}}},
  volume = {101},
  issn = {0002-8223},
  shorttitle = {Considerations in {{Planning Vegan Diets}}},
  doi = {10.1016/S0002-8223(01)00167-5},
  abstract = {This article reviews research on the growth and nutrient intake of vegan children and provides guidelines for counseling parents of vegan children. Although diets of vegan children meet or exceed recommendations for most nutrients, and vegan children have higher intakes of fiber and lower intakes of total fat, saturated fat, and cholesterol than omnivore children, some studies indicate that they may be low in calcium. In addition, bioavailability of zinc and iron from plant foods can be low. Protein needs are slightly higher for vegan children but are easily met with a varied diet that provides adequate energy. Special attention should be given to dietary practices that enhance absorption of zinc and iron from plant foods. Further, good sources of the $\varpi$-3 fatty acid linolenic acid should be emphasized to enhance synthesis of the long-chain fatty acid docosahexanoic acid. Dietetics professionals who counsel vegan families should help parents identify good sources of vitamin B-12, riboflavin, zinc, calcium and, if sun exposure is not adequate, vitamin D. This should not be problematic, due to the growing number and availability of fortified vegan foods that can help children meet all nutrient needs. Therefore, with appropriate food choices, vegan diets can be adequate for children at all ages. J Am Diet Assoc. 2001;101:661-669.},
  number = {6},
  urldate = {2017-11-13},
  url = {http://www.sciencedirect.com/science/article/pii/S0002822301001675},
  journal = {Journal of the American Dietetic Association},
  author = {Messina, Virginia and Reed Mangels, Ann},
  year = {2001},
  pages = {661-669},
  file = {/home/ivanhercaz/Zotero/storage/FS5MKEWA/Messina and Reed mangels - 2001 - Considerations in Planning Vegan Diets Children.html}
}

