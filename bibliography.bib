@article{tollebeekRenaissanceFossilizationMichelet2001,
  title = {`{{Renaissance}}' and `Fossilization': {{Michelet}}, {{Burckhardt}}, and {{Huizinga}}},
  volume = {15},
  issn = {1477-4658},
  shorttitle = {`{{Renaissance}}' and `Fossilization'},
  doi = {10.1111/1477-4658.00374},
  abstract = {From its very first use, the term `Renaissance' indicated change and renewal. Michelet, in his lectures at the Coll{\`e}ge de France in 1840\textendash{}1841, represented the Renaissance as `the formation of the modern world'. Burckhardt enshrined the term in The Civilization of the Renaissance in Italy (1860) as a term which focused on a turning point in history. However, as a result, the term also immediately required a conceptual counterpart. This was found in the term `fossilisation'. The most detailed vision of the `fossilisation' which allegedly preceded the new civilization of the Renaissance was provided by the Dutch historian Huizinga. In The Autumn of the Middle Ages (1919) he presented Burgundian civilization as a civilization in turmoil, where the old continued its insidious spread, with no meaning and no future. This dual conceptual device \textendash`Renaissance' and `Fossilisation'\textendash{} caused two problems. The first was related to the transition between the old and the new world: how could change and renewal emerge from a `fossilised' civilisation? The second problem concerned the status of the terms `Renaissance' and `Fossilisation'. After all, it rapidly became evident that the concept of `fossilisation' had its roots in cultural criticism, rather than in cultural history. Did something similar not apply to the term, a word which referred to a turning point which historians like Michelet, Burckhardt and Huizinga desired chiefly in the present?},
  language = {en},
  number = {3},
  urldate = {2017-02-12},
  url = {http://onlinelibrary.wiley.com.bibproxy.ulpgc.es/doi/10.1111/1477-4658.00374/abstract},
  journal = {Renaissance Studies},
  author = {Tollebeek, Jo},
  month = sep,
  year = {2001},
  pages = {354-366},
  file = {/home/ivanhercaz/Zotero/storage/A7DGS8BF/Tollebeek - 2001 - ‘Renaissance’ and ‘fossilization’ Michelet, Burck.pdf;/home/ivanhercaz/Zotero/storage/PPQCK3Z6/abstract.html}
}

